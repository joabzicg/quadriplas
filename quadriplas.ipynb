   "source": [
    "import numpy as np\n",
    "\n",
    "# Defina sua matriz\n",
    "matriz = np.array([[1, 2],\n",
    "                   [3, 4]])\n",
    "\n",
    "# Calcule a matriz inversa\n",
    "matriz_inversa = np.linalg.inv(matriz)\n",
    "\n",
    "print(\"Matriz original:\")\n",
    "print(matriz)\n",
    "\n",
    "print(\"\\nMatriz inversa:\")\n",
    "print(matriz_inversa)"
   ]
